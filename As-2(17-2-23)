#include <stdio.h>
int main()
{
    int i,m,l,num,j,n,even_sum=0,odd_sum=0;
    printf("enter the value of num:\n");
    scanf("%d",&num);
    printf("enter the value:\n");
    scanf("%d",&l);
    for(i=1;i<=num;i++)
    if(i%l==0)
    printf("%d\t",i);
    printf("enter n value:\n");
    scanf("%d",&m);
    for(j=1;j<=m;j++)
    {
        if(j%2==0)
        even_sum=even_sum+j;
        else
        odd_sum=odd_sum+j;
    }
    printf("sum of all odd numbers=%d\n",odd_sum);
    printf("sum of all even numbers=%d\n",even_sum);
    return 0;
}
