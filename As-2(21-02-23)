// Armstrong program in C
#include<stdio.h> 
int main ()
{
    int num,r,sum=0,temp;
    scanf("%d",&num);
    temp = num;
    while(num>0)
    {
        r=num%10;
        sum=sum+(r*r*r);
        num=num/10;
    }
    if(temp==sum)
        printf("%d is Armstrong\n",temp);
    else
        printf("%d is Not Armstrong\n",temp);

}
